User
    name Text
    email Text
    password Text Maybe
    gender Text
    tribe TribeId
    facebook_id Text Maybe defaul=NULL
    accepted_terms Bool default=false
    UniqueEmail email
    deriving Typeable

Tribe
    title Text
    daysOfWeek [Int]
    latitude Double
    longitude Double
    timezone Text
    UniqueTitle title

Location
    title Text
    latitude Double
    longitude Double
    standard Bool default=false
    tribe TribeId
    UniqueTitleTribe title tribe

Workout
    title Text
    description Text Maybe default=NULL
    reps Int Maybe default=NULL
    time Int Maybe default=NULL
    standard Bool default=false
    allowUserReps Bool
    allowUserTime Bool
    tribe TribeId


