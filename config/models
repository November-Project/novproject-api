User
    name Text
    email Text
    password Text Maybe
    gender Text
    tribe TribeId
    facebookId Text Maybe
    acceptedTerms Bool
    UniqueUserEmail email
    deriving Typeable

Tribe
    title Text
    daysOfWeek [Int]
    latitude Double
    longitude Double
    timezone Text
    UniqueTribeTitle title

Location
    title Text
    latitude Double
    longitude Double
    standard Bool
    tribe TribeId
    UniqueLocationTitleTribe title tribe

Workout
    title Text
    description Text
    reps Int
    time Int
    standard Bool
    allowUserReps Bool
    allowUserTime Bool
    tribe TribeId
    UniqueWorkoutTitleTribe title tribe

Schedule
    time Text Maybe
    dayOfWeek Int
    tribe TribeId
    location LocationId Maybe
    UniqueScheduleDayTribe dayOfWeek tribe

Event
    date UTCTime
    tribe TribeId
    location LocationId
    workout WorkoutId
    UniqueEventDateTribe date tribe

Verbal
    user UserId
    event EventId
    UniqueVerbalUserEvent user event

Result
    user UserId
    event EventId
    reps Int
    time Int
    pr Bool
    UniqueResultUserEvent user event
