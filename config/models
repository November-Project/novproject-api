User sql=users
    name Text
    email Text
    password Text Maybe
    gender Text
    tribe TribeId
    photoUrl Text Maybe
    facebookId Text Maybe
    acceptedTerms Bool
    verifyKey Text Maybe
    isVerified Bool default=false
    forgotToken Text Maybe
    isAdmin Bool
    tribeAdmin TribeId Maybe
    usesBcrypt Bool default=true
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueUserEmail email
    UniqueUserFacebookId facebookId !force

Tribe sql=tribes
    title Text
    daysOfWeek [Int] default="'[3]'"
    latitude Double
    longitude Double
    timezone Text
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueTribeTitle title

Location sql=locations
    title Text
    latitude Double
    longitude Double
    standard Bool
    tribe TribeId
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueLocationTitleTribe title tribe

Workout sql=workouts
    title Text
    description Text
    reps Double
    time Int
    standard Bool
    allowUserReps Bool
    allowUserTime Bool
    allowUserPr Bool
    tribe TribeId
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueWorkoutTitleTribe title tribe

Event sql=events
    title Text Maybe
    tribe TribeId
    date Day
    times [TimeOfDay]
    hideWorkout Bool
    location LocationId Maybe
    workout WorkoutId Maybe
    tags [Tag] default="'[]'"
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueEventTribeDate tribe date

Recurring sql=recurring
    title Text Maybe
    tribe TribeId
    times [TimeOfDay]
    week Int
    days [Int]
    hideWorkout Bool
    location LocationId Maybe
    workout WorkoutId Maybe
    tags [Tag] default="'[]'"
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly

Verbal sql=verbals
    user UserId
    date Day
    tribe TribeId
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueVerbalUserDate user date

Result sql=results
    user UserId
    event EventId
    eventTime Text
    reps Double
    time Int
    pr Bool
    createdAt UTCTime default=now() MigrationOnly
    updatedAt UTCTime default=now() MigrationOnly
    UniqueResultUserEventEventTime user event eventTime
