User sql=users
    name Text
    email Text
    password Text Maybe
    gender Text
    tribe TribeId
    facebookId Int Maybe
    acceptedTerms Bool
    verifyKey Text Maybe
    isVerified Bool default=false
    forgotToken Text Maybe
    isAdmin Bool
    tribeAdmin TribeId Maybe
    UniqueUserEmail email

Session sql=sessions
    user UserId
    token Text
    deviceInfo Text Maybe
    expired Bool default=false
    UniqueSessionToken token

Tribe sql=tribes
    title Text
    latitude Double
    longitude Double
    timezone Text
    UniqueTribeTitle title

Location sql=locations
    title Text
    latitude Double
    longitude Double
    standard Bool
    tribe TribeId
    UniqueLocationTitleTribe title tribe

Workout sql=workouts
    title Text
    description Text
    reps Double
    time Int
    standard Bool
    allowUserReps Bool
    allowUserTime Bool
    tribe TribeId
    UniqueWorkoutTitleTribe title tribe

Schedule sql=schedules
    time Text Maybe
    dayOfWeek Int
    tribe TribeId
    location LocationId Maybe
    UniqueScheduleDayTribe dayOfWeek tribe

Event sql=events
    date UTCTime
    tribe TribeId
    location LocationId Maybe
    workout WorkoutId Maybe
    UniqueEventDateTribe date tribe

Verbal sql=verbals
    user UserId
    event EventId
    UniqueVerbalUserEvent user event

Result sql=results
    user UserId
    event EventId
    reps Double
    time Int
    pr Bool
    UniqueResultUserEvent user event
